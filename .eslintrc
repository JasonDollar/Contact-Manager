{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
    
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "16.0",
      "flowVersion": "0.53"
    },
    "jsx": true,
    "propWrapperFunctions": ["forbidExtraProps"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-unused-vars": ["warn"],
    "react/prop-types": ["off"],
    "no-console": ["off"],
    "linebreak-style": "off",
    "indent": ["error", 2, {"ignoredNodes": ["JSXElement"]}],
    "react/prefer-stateless-function": "warn"
  }

}